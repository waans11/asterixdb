use dataverse test;
set import-private-functions 'true';

for $o in dataset('DBLP')
let $jacc := similarity-jaccard-check(gram-tokens($o.nested.title, 3, false), gram-tokens("Transactions for Cooperative Environments", 3, false), 0.5f)
where $jacc[0]
return $o.nested
