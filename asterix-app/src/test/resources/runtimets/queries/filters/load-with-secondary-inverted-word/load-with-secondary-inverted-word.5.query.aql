use dataverse test;

for $m in dataset('FacebookMessages')
where similarity-jaccard(word-tokens($m.message), word-tokens("love sprint at&t verizon")) >= 0.2f
and $m.send-time < datetime("2012-12-20T10:10:00")
order by $m.send-time
return $m
